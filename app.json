{
  "expo": {
    "name": "Project Zephyrus",
    "slug": "project-zephyrus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.anonymous.projectzephyrus"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4e8bd87d-1fe3-4e7c-8b89-c2c516c8d567"
      }
    }
  }
}
